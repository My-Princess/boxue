.clearfix:after,
.clearfix:before {
  content: '';
  display: table;
}
.clearfix:after {
  clear: both;
}
.activeColor {
  color: #db1f13 !important;
}
.collapse-son:first-child {
  padding-top: 38rpx;
}
.collapse-son {
  padding: 24rpx 0;
  border-bottom: 1rpx solid #eeeeee;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
}
.collapse-son text {
  font-family: PingFang SC;
  font-weight: 500;
}
.collapse-son-size {
  font-size: 30rpx;
  color: #333333;
}
.collapse-son-r {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: absolute;
  right: 53rpx;
}
.collapse-son-r-fraction {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #db7a13;
}
.collapse-son-r-icon {
  display: block;
  width: 42rpx;
  height: 42rpx;
  margin-left: 20rpx;
  background: #e4e4e4;
}
.collapse-son::before {
  content: '';
  width: 30rpx;
  height: 30rpx;
  background: white;
  border: 2px solid #eeeeee;
  border-radius: 50%;
  position: absolute;
  left: -40rpx;
  -webkit-transform-origin: 0;
          transform-origin: 0;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  z-index: 1;
  font-size: 24rpx;
}
.mytask-body-wu {
  text-align: center;
  margin-top: 100rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}
.mytask-body-c {
  margin-top: 48rpx;
  padding-left: 60rpx;
}
.mytask-body-c .u-node {
  border-radius: 100rpx;
}
.mytask-body-c .u-node .uicon-pushpin-fill {
  height: 42rpx;
  width: 42rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-radius: 100rpx;
}

