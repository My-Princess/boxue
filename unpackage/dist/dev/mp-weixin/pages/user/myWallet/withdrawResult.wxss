.clearfix:after,
.clearfix:before {
  content: '';
  display: table;
}
.clearfix:after {
  clear: both;
}
.activeColor {
  color: #db1f13 !important;
}
.result-body-box {
  margin: 0 40rpx;
  box-shadow: 0 0 2px 0 #CACACA;
  border-radius: 10rpx;
}
.result-body-list {
  padding-top: 60rpx;
  margin: 0 40rpx;
}
.result-body-list-item {
  border-top: 1rpx solid #EEEEEE;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 20rpx 0;
}
.result-body-list-item-l {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}
.result-body-list-item-r {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}
.result-body-c {
  padding-top: 60rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.result-body-c image {
  display: block;
  width: 132rpx;
  height: 132rpx;
}
.result-body-c-name {
  padding-top: 40rpx;
  padding-bottom: 20rpx;
  font-size: 48rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}
.result-body-c-txt {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}
.result-body-btn {
  margin: auto;
  margin-top: 120rpx;
  width: 600rpx;
  height: 98rpx;
  background: #DC2A1F;
  border-radius: 49px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}

