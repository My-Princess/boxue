.clearfix:after,
.clearfix:before {
  content: '';
  display: table;
}
.clearfix:after {
  clear: both;
}
.activeColor {
  color: #db1f13 !important;
}
.withdraw-box {
  margin: 0 40rpx;
  box-shadow: 0 0 6px 0 #CACACA;
  border-radius: 10px;
}
.withdraw-box-top {
  height: 124rpx;
  background: #F6F6F6;
  border-radius: 10px;
  padding-left: 20rpx;
  padding-top: 60rpx;
}
.withdraw-box-top text {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}
.withdraw-box-top text:nth-child(2) {
  margin-left: 20rpx;
  color: #2440B3;
}
.withdraw-box-c {
  padding: 30rpx 20rpx 40rpx;
}
.withdraw-box-c-name {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}
.withdraw-box-c-line {
  width: 100%;
  height: 1rpx;
  background: #EEEEEE;
  margin-bottom: 8rpx;
}
.withdraw-box-c-tip {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #DB1F13;
}
.withdraw-box-c-btn {
  margin-top: 114rpx;
  width: 600rpx;
  height: 98rpx;
  background: #EEEEEE;
  border-radius: 50px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}
.withdraw-box-c-money {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.withdraw-box-c-money-l {
  font-size: 72rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}
.withdraw-box-c-money-r {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.withdraw-box-c-money-r input {
  width: 100%;
  height: 100%;
  font-size: 72rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}

