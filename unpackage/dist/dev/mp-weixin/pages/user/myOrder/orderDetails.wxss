.clearfix:after,
.clearfix:before {
  content: '';
  display: table;
}
.clearfix:after {
  clear: both;
}
.activeColor {
  color: #db1f13 !important;
}
.orderdetails-c {
  padding-top: 114rpx;
}
.orderdetails-c-list {
  margin: 40rpx 40rpx 0;
  padding: 40rpx 30rpx;
  box-shadow: 0 0 4px 1px rgba(0, 0, 0, 0.1);
  border-radius: 10px;
}
.orderdetails-c-list-item:nth-child(1) {
  padding-top: 0;
}
.orderdetails-c-list-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 20rpx 0;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}
.orderdetails-c-list-item1 {
  padding: 20rpx 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.orderdetails-c-list-item1-name {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}
.orderdetails-c-list-item1-txt {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #DB1F13;
}
.orderdetails-c-list-line {
  width: 100%;
  height: 1rpx;
  background: #EEEEEE;
}
.orderdetails-c-list-pay {
  padding-top: 20rpx;
  text-align: right;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 800;
  color: #333333;
}

