.clearfix:after,
.clearfix:before {
  content: '';
  display: table;
}
.clearfix:after {
  clear: both;
}
.activeColor {
  color: #db1f13 !important;
}
.successful-c {
  position: relative;
}
.successful-list {
  padding: 60rpx 40rpx 0;
}
.successful-list-name {
  font-size: 35rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #666666;
  padding-bottom: 10rpx;
}
.successful-list-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 25rpx 18rpx;
  border-bottom: 1rpx solid #EEEEEE;
}
.successful-list-item-name {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}
.successful-list-item-r {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #666666;
}
.successful-list-item-r-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.successful-list-item-r-box .icon-qiehuan- {
  font-size: 34rpx;
  padding: 0 8rpx;
}
.successful-title image {
  display: block;
  width: 192rpx;
  height: 192rpx;
  background: #dc2a1f;
  border-radius: 50%;
  position: relative;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.successful-title-name {
  font-size: 35rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  margin-top: 50rpx;
  margin-bottom: 30rpx;
  text-align: center;
}
.successful-title-txt {
  padding-bottom: 6rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  text-align: center;
}

