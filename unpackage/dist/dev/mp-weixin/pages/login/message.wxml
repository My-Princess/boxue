<view class="message"><view class="message-logo"><image src="https://boxue-resource.oss-cn-shenzhen.aliyuncs.com/tie/logo.png" mode></image></view><view class="message-phone"><view data-event-opts="{{[['tap',[['areacode',['$event']]]]]}}" class="message-phone-left" bindtap="__e"><view class="message-phone-left-size">{{"+"+areacodenumber}}</view><view class="iconfont icon-xiajiantou"></view></view><input class="message-content" type="text" value="{{phonevlaue}}" data-event-opts="{{[['input',[['__set_model',['','phonevlaue','$event',[]]],['messageInput',['$event']]]]]}}" bindinput="__e"/></view><view class="message-line"></view><view class="message-yzm"><input type="text" placeholder="{{login.codeTip}}" data-event-opts="{{[['input',[['__set_model',['','code','$event',[]]],['messageInput',['$event']]]]]}}" value="{{code}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['qqmessages',['$event']]]]]}}" class="message-yzm-hq" bindtap="__e"><text>{{word}}</text></view></view><view class="message-line"></view><view class="message-message"><view data-event-opts="{{[['tap',[['messages',['$event']]]]]}}" class="{{['message-btn',(cahngeColor)?'cahngeColor':'']}}" bindtap="__e"><text>{{login.login}}</text></view></view><view class="message-info"><view class="message-info-left">{{login.rexy}}</view><view data-event-opts="{{[['tap',[['agreement',['$event']]]]]}}" class="message-info-right" bindtap="__e">{{"《"+login.privacy+"》"}}</view></view></view>