.clearfix:after,
.clearfix:before {
  content: '';
  display: table;
}
.clearfix:after {
  clear: both;
}
.activeColor {
  color: #db1f13 !important;
}
 .integral-top-r image {
  width: 44rpx;
  height: 40rpx;
}
.mycourses-flex {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: #fff;
  z-index: 99;
}
.mycourses-tab {
  height: 100rpx;
  padding-top: 30rpx;
}
.mycourses-body {
  padding: 0 40rpx;
  padding-top: 120rpx;
}
.mycourses-body-c-item {
  padding-top: 40rpx;
}
.mycourses-body-c-item-b {
  margin-top: 16rpx;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}
.mycourses-body-c1 {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  margin-top: 40rpx;
}
.mycourses-body-c1-item:nth-child(2n) {
  margin-left: 20rpx;
}
.mycourses-body-c1-item {
  margin-bottom: 20rpx;
  width: 324rpx;
  background: #ffffff;
  border-radius: 10px;
  box-shadow: 0px 0px 6px 0 #eeeeee;
}
.mycourses-body-c1-item image {
  margin-bottom: 18rpx;
  display: block;
  width: 324rpx;
  height: 193rpx;
  border-radius: 10px;
}
.mycourses-body-c1-item-name {
  padding-left: 15rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 800;
  color: #333333;
}
.mycourses-body-c1-item-txt {
  padding-left: 15rpx;
  padding-bottom: 20rpx;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}
.mycourses-body-zw {
  text-align: center;
  font-family: PingFang SC;
  font-weight: 500;
}
.mycourses-body-zw-tip {
  margin-top: 160rpx;
  font-size: 26rpx;
  color: #999999;
}
.mycourses-body-zw-m {
  margin-top: 60rpx;
  font-size: 28rpx;
  color: #db1f13;
}
.mycourses-body-nodata {
  text-align: center;
  padding-top: 20rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}

