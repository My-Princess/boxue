.clearfix:after,
.clearfix:before {
  content: '';
  display: table;
}
.clearfix:after {
  clear: both;
}
.activeColor {
  color: #db1f13 !important;
}
.blank {
  font-size: 28rpx;
  text-align: center;
  padding-top: 100rpx;
}
.complete-tab-box-sp-item {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-bottom: 40rpx;
}
.complete-tab-box-sp-item-left {
  width: 208rpx;
  height: 137rpx;
  padding-right: 31rpx;
}
.complete-tab-box-sp-item-left image {
  width: 100%;
  height: 100%;
  border-radius: 10px;
}
.complete-tab-box-sp-item-right {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.complete-tab-box-sp-item-right-title {
  width: 60%;
  padding-top: 6rpx;
  padding-bottom: 10rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 800;
  color: #333333;
}
.complete-tab-box-sp-item-right-text {
  width: 85%;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #666666;
}
.complete-tab-box-sp-item-right-bottom {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: end;
  -webkit-align-items: flex-end;
          align-items: flex-end;
  padding-top: 8rpx;
}
.complete-tab-box-sp-item-right-bottom text {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #db1f13;
}
.complete-tab-box-sp-item-right-bottom view text {
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}

