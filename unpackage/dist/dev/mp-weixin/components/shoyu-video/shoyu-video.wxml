<view><block wx:if="{{videoUri}}"><video class="video" id="myVideo" src="{{videoUri}}" poster="https://boxue-resource.oss-cn-shenzhen.aliyuncs.com/tie/videoFooter.png" autoplay="{{true}}" controls="{{true}}" data-event-opts="{{[['play',[['play',['$event']]]],['pause',[['pause',['$event']]]]]}}" bindplay="__e" bindpause="__e"><cover-view class="iconfont icon-zuojiantou"></cover-view><cover-view data-event-opts="{{[['touchstart',[['mouseover',['$event']]]],['mouseout',[['mouseout',['$event']]]]]}}" hidden="{{!(!showTip)}}" class="video-control" bindtouchstart="__e" bindmouseout="__e"><cover-view class="video-title"><cover-view class="video-nmae">{{title}}</cover-view></cover-view><cover-view data-event-opts="{{[['tap',[['showdistinct',['$event']]]]]}}" class="distinct" bindtap="__e">{{distinctname}}</cover-view><cover-view data-event-opts="{{[['tap',[['showSwitchRate',['$event']]]]]}}" class="multi rate" bindtap="__e">{{"x "+currentRate}}</cover-view></cover-view><cover-view class="{{['multi-list rate',(rateShow)?'active':'']}}"><block wx:for="{{['0.5','0.8','1.0','1.25','1.5']}}" wx:for-item="item" wx:for-index="index" wx:key="index"><cover-view class="{{['multi-item rate',(item==currentRate)?'active':'']}}" data-rate="{{item}}" data-event-opts="{{[['tap',[['switchRate',['$event']]]]]}}" bindtap="__e">{{''+item+''}}</cover-view></block></cover-view><cover-view class="{{['distinct-list',(distinctShow)?'dactive':'']}}"><block wx:for="{{distinctData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><cover-view class="{{['distinct-list-item rate',(item.name==distinctname)?'active':'']}}" data-name="{{item.name}}" data-type="{{item.type}}" data-event-opts="{{[['tap',[['distinctBtn',['$event']]]]]}}" bindtap="__e">{{''+item.name+''}}</cover-view></block></cover-view></video></block></view>